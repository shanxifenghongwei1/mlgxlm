.page {
  width: 100%;
  min-height: 100vh;
  background: #F0F0F0;
  overflow: hidden;
}
.detail {
  width: 100%;
  height: 292rpx;
  padding: 0 3%;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.detail .detail-box {
    width: 100%;
    height: 100%;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: cover;
    padding: 0 30rpx;
    box-sizing: border-box;
}
.detail .detail-box .title {
      width: 100%;
      height: 106rpx;
      font-size: 30rpx;
      text-align: center;
      line-height: 106rpx;
      color: #666666;
}
.detail .detail-box .con {
      width: 100%;
      height: 186rpx;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -webkit-flex-direction: column;
              flex-direction: column;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
              justify-content: center;
      -webkit-box-align: start;
      -webkit-align-items: flex-start;
              align-items: flex-start;
      font-size: 24rpx;
}
.detail .detail-box .con .con-box {
        width: 100%;
        height: 24rpx;
        padding: 8rpx 0;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 1;
        -webkit-box-orient: vertical;
        autoprefixer: on;
}
.detail .detail-box .con .con-box text:nth-child(1) {
          color: #B2B2B2;
}
.detail .detail-box .con .con-box text:nth-child(2) {
          color: #666666;
          margin-left: 3rpx;
}
.method {
  width: 100%;
  margin-top: 30rpx;
  background: #ffffff;
  position: relative;
}
.method .method-box {
    width: 100%;
    height: 100rpx;
    padding: 0 5%;
    box-sizing: border-box;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.method .left-pic {
    width: 60rpx;
    height: 60rpx;
    padding: 20rpx;
}
.method .con {
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
            flex-grow: 1;
    height: 100%;
    padding-left: 10rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    text-align: left;
    line-height: 100rpx;
}
.method .right-pic {
    width: 40rpx;
    height: 40rpx;
    padding: 20rpx;
}
.method:after {
  content: "";
  display: block;
  position: absolute;
  width: 100%;
  height: 2rpx;
  left: 0;
  bottom: 50%;
  background: #C7C7C7;
}
.pay {
  width: 100%;
  padding: 0 5%;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 50rpx;
}

