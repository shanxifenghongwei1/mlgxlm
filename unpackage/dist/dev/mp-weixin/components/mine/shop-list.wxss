          align-items: center;
}
.cons {
  overflow: hidden;
  width: 94%;
  padding: 0 3%;
}
.any-over {
  overflow: hidden;
}
.font-weig {
  font-weight: 600;
}
.page {
  width: 100%;
  min-height: 100vh;
  background: #F0F0F0;
  overflow: hidden;
}
.shop-father .goods {
  width: 100%;
  padding: 0 3%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.shop-father .shop {
  margin-top: 20rpx;
  width: 100%;
  height: 240rpx;
  border-radius: 20rpx;
  border: 2rpx solid #f0f0f0;
  border-bottom: 4rpx solid #FE0000;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
      -ms-flex-align: start;
          align-items: flex-start;
}
.shop-father .shop .shop-img {
    width: 20%;
    height: 80%;
}
.shop-father .shop .shop-img image {
      width: 100%;
      height: 100%;
}
.shop-father .shop .shop-message {
    width: 48%;
    height: 96%;
    padding: 2% 0 2% 5%;
    position: relative;
}
.shop-father .shop .shop-message .price-new {
      font-size: 30rpx;
      font-weight: 600;
      color: #FE0000;
}
.shop-father .shop .shop-message .price-new .noscripe {
        margin-left: 20rpx;
        text-decoration: line-through;
        font-size: 20rpx;
        color: #666;
}
.shop-father .shop .shop-message .shop-name {
      color: #FE0000;
      font-size: 30rpx;
      -webkit-box-align: end;
      -webkit-align-items: flex-end;
          -ms-flex-align: end;
              align-items: flex-end;
}
.shop-father .shop .shop-message .shop-name .shop-name-left {
        max-width: 40%;
        height: 40rpx;
        overflow: hidden;
        -o-text-overflow: ellipsis;
           text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 1;
        -webkit-box-orient: vertical;
        autoprefixer: on;
}
.shop-father .shop .shop-message .shop-name .addres {
        font-size: 20rpx;
        margin-left: 2rpx;
        color: #666;
}
.shop-father .shop .shop-message .label .tex {
      padding: 4rpx;
      font-size: 20rpx;
      color: #7f7f7f;
      border: 2rpx solid #7f7f7f;
}
.shop-father .shop .shop-message .goods-price {
      font-size: 20rpx;
}
.shop-father .shop .shop-message .goods-price text {
        margin: 0 10rpx;
        color: #FE0000;
        font-size: 30rpx;
}
.shop-father .shop .shop-message .goodsname {
      width: 70%;
      height: 35rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      display: -webkit-box;
      -webkit-line-clamp: 1;
      -webkit-box-orient: vertical;
      autoprefixer: on;
      overflow: hidden;
      font-size: 24rpx;
}
.shop-father .shop .shop-message .goodsname text {
        color: #FE0000;
        margin-right: 16rpx;
}
.shop-father .shop .shop-message .propergoods {
      font-size: 24rpx;
}
.shop-father .shop .shop-message .propergoods text {
        color: #FE0000;
        margin-right: 16rpx;
}
.shop-father .shop .show-box {
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
        -ms-flex-positive: 1;
            flex-grow: 1;
    height: 100%;
    padding: 20rpx 20rpx 20rpx 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.shop-father .shop .show-box .box {
      width: 100%;
      height: 100%;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -webkit-flex-direction: column;
          -ms-flex-direction: column;
              flex-direction: column;
      -webkit-box-align: end;
      -webkit-align-items: flex-end;
          -ms-flex-align: end;
              align-items: flex-end;
}
.shop-father .shop .show-box .box .pintuan,
      .shop-father .shop .show-box .box .newPeople {
        display: inline-block;
        padding: 3rpx 10rpx;
        border: 1rpx solid #FE0000;
        font-size: 20rpx;
        border-radius: 15rpx;
        margin-bottom: 10rpx;
        color: #FE0000;
}
.shop-father .advert {
  width: 100%;
  height: 200rpx;
  background: pink;
  margin: 20rpx 0;
}

