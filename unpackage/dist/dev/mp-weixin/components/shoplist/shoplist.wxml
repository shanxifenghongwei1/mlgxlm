<view><block wx:for="{{shoplists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['shoplists','',index,'id']]]]]]]}}" class="shop-father" bindtap="__e"><view class="shop"><view class="shop-img"><image src="{{item.image}}" mode="widthFix"></image></view><view class="shop-message"><view class="shop-name"><text>{{item.shopname}}</text><text class="addres">{{item.address}}</text></view><view class="shop-start"><uni-rate vue-id="{{'1-'+index}}" disabled="{{false}}" margin="2" size="10" max="5" value="{{item.start}}" color="#7f7f7f" active-color="#ffb540" data-event-opts="{{[['^change',[['onChange']]]]}}" bind:change="__e" bind:__l="__l"></uni-rate></view><block wx:if="{{myid!=4}}"><view><block wx:if="{{myid!=4}}"><view class="label"><text class="tex">{{item.labels}}</text></view></block><view class="goods-price">原价<text>{{item.goodssprice}}</text>平台价<text>{{item.newgoodssprice}}</text></view><view class="goodsname"><text class="iconfont icon-xiangmu"></text>{{''+item.goodsname}}</view><view class="propergoods"><text class="iconfont icon-tuan"></text><text class="mon">{{item.othergoodsprices+"元"}}</text>{{''+item.othergoods+''}}</view></view></block><block wx:else><view><view class="goodsname"><text class="iconfont icon-xiangmu"></text>{{''+item.goodsname}}</view><view class="any-flex"><view class="progres"><cmd-progress vue-id="{{'2-'+index}}" stroke-width="8" stroke-color="#FE0000" percent="{{30}}" bind:__l="__l"></cmd-progress></view><view class="havepeoples">已拼<text>{{item.prople}}</text>位</view></view><view class="price-new">￥ 199.9<text class="noscripe">￥ 330</text></view></view></block><block wx:if="{{myid==1}}"><text class="gobuy">去拼团</text></block><block wx:if="{{myid==2||myid==3||myid==5}}"><text class="gobuy">进店</text></block><block wx:if="{{myid==4}}"><text data-event-opts="{{[['tap',[['go',['$event']]]]]}}" class="gobuy" bindtap="__e">马上抢</text></block><block wx:if="{{myid==1}}"><text class="gobuy">去拼团</text></block><block wx:if="{{myid==6}}"><text class="gobuy">更多团购</text></block><block wx:if="{{myid==6}}"><view class="havepeople">已售<text>{{item.prople}}</text>位</view></block><block wx:if="{{myid==1}}"><view class="havepeople">已拼<text>{{item.prople}}</text>位</view></block></view></view></view></block></view>